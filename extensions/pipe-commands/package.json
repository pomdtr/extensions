{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "pipe-commands",
  "title": "Pipe commands",
  "description": "Pass your selection to any scripts.",
  "icon": "command-icon.png",
  "author": "pomdtr",
  "license": "MIT",
  "commands": [
    {
      "name": "pipe-selection-to-command",
      "title": "Pipe Selection to Command",
      "subtitle": "Pipe command",
      "description": "Take Action on the Selection.",
      "mode": "view"
    },
    {
      "name": "pipe-clipboard-to-command",
      "title": "Pipe Clipboard to Command",
      "subtitle": "Pipe command",
      "description": "Take Action on the Clipboard.",
      "mode": "view"
    },
    {
      "name": "create-pipe-command",
      "title": "Create Pipe command",
      "subtitle": "Pipe command",
      "description": "Generate a new pipe command template",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.27.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
"@typescript-eslint/parser": "^5.0.0",
"eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}
